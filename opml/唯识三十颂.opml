<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
	<head>
		<title>唯识三十颂</title>
	</head>
	<body>
		<outline text="甲一、明唯识相" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B2%E4%B8%80%E3%80%81%E6%98%8E%E5%94%AF%E8%AF%86%E7%9B%B8%3C/span%3E" _heading="2">
		<outline text="乙一、略标" _mubu_text="%E4%B9%99%E4%B8%80%E3%80%81%E7%95%A5%E6%A0%87">
		<outline text="丙一、释难破执" _mubu_text="%E4%B8%99%E4%B8%80%E3%80%81%E9%87%8A%E9%9A%BE%E7%A0%B4%E6%89%A7">
		<outline text="由假说我法，有种种相转。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B1%E5%81%87%E8%AF%B4%E6%88%91%E6%B3%95%EF%BC%8C%E6%9C%89%E7%A7%8D%E7%A7%8D%E7%9B%B8%E8%BD%AC%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="丙二、标宗归识" _mubu_text="%E4%B8%99%E4%BA%8C%E3%80%81%E6%A0%87%E5%AE%97%E5%BD%92%E8%AF%86">
		<outline text="彼依识所变。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BD%BC%E4%BE%9D%E8%AF%86%E6%89%80%E5%8F%98%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text=" 丙三、彰能变体" _mubu_text="%20%E4%B8%99%E4%B8%89%E3%80%81%E5%BD%B0%E8%83%BD%E5%8F%98%E4%BD%93">
		<outline text="此能变唯三，谓异熟、思量，及了别境识。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AD%A4%E8%83%BD%E5%8F%98%E5%94%AF%E4%B8%89%EF%BC%8C%E8%B0%93%E5%BC%82%E7%86%9F%E3%80%81%E6%80%9D%E9%87%8F%EF%BC%8C%E5%8F%8A%E4%BA%86%E5%88%AB%E5%A2%83%E8%AF%86%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text="乙二、广释" _mubu_text="%E4%B9%99%E4%BA%8C%E3%80%81%E5%B9%BF%E9%87%8A">
		<outline text="丙一、明能变相" _mubu_text="%E4%B8%99%E4%B8%80%E3%80%81%E6%98%8E%E8%83%BD%E5%8F%98%E7%9B%B8">
		<outline text="丁一、异熟能变" _mubu_text="%E4%B8%81%E4%B8%80%E3%80%81%E5%BC%82%E7%86%9F%E8%83%BD%E5%8F%98">
		<outline text="戊一、三相门	" _mubu_text="%E6%88%8A%E4%B8%80%E3%80%81%E4%B8%89%E7%9B%B8%E9%97%A8%09">
		<outline text="初阿赖耶识，异熟一切种。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%88%9D%E9%98%BF%E8%B5%96%E8%80%B6%E8%AF%86%EF%BC%8C%E5%BC%82%E7%86%9F%E4%B8%80%E5%88%87%E7%A7%8D%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊二、所缘行相门" _mubu_text="%E6%88%8A%E4%BA%8C%E3%80%81%E6%89%80%E7%BC%98%E8%A1%8C%E7%9B%B8%E9%97%A8">
		<outline text="不可知；执受、处、了。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%8F%AF%E7%9F%A5%EF%BC%9B%E6%89%A7%E5%8F%97%E3%80%81%E5%A4%84%E3%80%81%E4%BA%86%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊三、心所相应门" _mubu_text="%E6%88%8A%E4%B8%89%E3%80%81%E5%BF%83%E6%89%80%E7%9B%B8%E5%BA%94%E9%97%A8">
		<outline text="常与触、作意、受、想、思相应。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B8%B8%E4%B8%8E%E8%A7%A6%E3%80%81%E4%BD%9C%E6%84%8F%E3%80%81%E5%8F%97%E3%80%81%E6%83%B3%E3%80%81%E6%80%9D%E7%9B%B8%E5%BA%94%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊四、五受相应门" _mubu_text="%E6%88%8A%E5%9B%9B%E3%80%81%E4%BA%94%E5%8F%97%E7%9B%B8%E5%BA%94%E9%97%A8">
		<outline text="唯舍受。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%94%AF%E8%88%8D%E5%8F%97%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊五、三性分别门" _mubu_text="%E6%88%8A%E4%BA%94%E3%80%81%E4%B8%89%E6%80%A7%E5%88%86%E5%88%AB%E9%97%A8">
		<outline text="是无覆无记。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%98%AF%E6%97%A0%E8%A6%86%E6%97%A0%E8%AE%B0%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊六、心所例同门" _mubu_text="%E6%88%8A%E5%85%AD%E3%80%81%E5%BF%83%E6%89%80%E4%BE%8B%E5%90%8C%E9%97%A8">
		<outline text="触等亦如是。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%A7%A6%E7%AD%89%E4%BA%A6%E5%A6%82%E6%98%AF%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊七、因果譬喻门" _mubu_text="%E6%88%8A%E4%B8%83%E3%80%81%E5%9B%A0%E6%9E%9C%E8%AD%AC%E5%96%BB%E9%97%A8">
		<outline text="恒转如瀑流。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%81%92%E8%BD%AC%E5%A6%82%E7%80%91%E6%B5%81%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊八、伏断位次门" _mubu_text="%E6%88%8A%E5%85%AB%E3%80%81%E4%BC%8F%E6%96%AD%E4%BD%8D%E6%AC%A1%E9%97%A8">
		<outline text="阿罗汉位舍。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%98%BF%E7%BD%97%E6%B1%89%E4%BD%8D%E8%88%8D%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text="丁二、思量能变" _mubu_text="%E4%B8%81%E4%BA%8C%E3%80%81%E6%80%9D%E9%87%8F%E8%83%BD%E5%8F%98">
		<outline text="戊一、举体出名门" _mubu_text="%E6%88%8A%E4%B8%80%E3%80%81%E4%B8%BE%E4%BD%93%E5%87%BA%E5%90%8D%E9%97%A8">
		<outline text="次第二能变，是识名末那。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AC%A1%E7%AC%AC%E4%BA%8C%E8%83%BD%E5%8F%98%EF%BC%8C%E6%98%AF%E8%AF%86%E5%90%8D%E6%9C%AB%E9%82%A3%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊二、所依门" _mubu_text="%E6%88%8A%E4%BA%8C%E3%80%81%E6%89%80%E4%BE%9D%E9%97%A8">
		<outline text="依彼转。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BE%9D%E5%BD%BC%E8%BD%AC%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊三、所缘门" _mubu_text="%E6%88%8A%E4%B8%89%E3%80%81%E6%89%80%E7%BC%98%E9%97%A8">
		<outline text="缘彼。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%BC%98%E5%BD%BC%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊四、体性行相门" _mubu_text="%E6%88%8A%E5%9B%9B%E3%80%81%E4%BD%93%E6%80%A7%E8%A1%8C%E7%9B%B8%E9%97%A8">
		<outline text="思量为性相。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%80%9D%E9%87%8F%E4%B8%BA%E6%80%A7%E7%9B%B8%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊五、心所相应门" _mubu_text="%E6%88%8A%E4%BA%94%E3%80%81%E5%BF%83%E6%89%80%E7%9B%B8%E5%BA%94%E9%97%A8">
		<outline text="四烦恼常俱，谓我痴我见，幷我慢我爱，及余触等俱。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%9B%9B%E7%83%A6%E6%81%BC%E5%B8%B8%E4%BF%B1%EF%BC%8C%E8%B0%93%E6%88%91%E7%97%B4%E6%88%91%E8%A7%81%EF%BC%8C%E5%B9%B7%E6%88%91%E6%85%A2%E6%88%91%E7%88%B1%EF%BC%8C%E5%8F%8A%E4%BD%99%E8%A7%A6%E7%AD%89%E4%BF%B1%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊六、三性分别门" _mubu_text="%E6%88%8A%E5%85%AD%E3%80%81%E4%B8%89%E6%80%A7%E5%88%86%E5%88%AB%E9%97%A8">
		<outline text="有覆无记摄。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%9C%89%E8%A6%86%E6%97%A0%E8%AE%B0%E6%91%84%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊七、界系分别门" _mubu_text="%E6%88%8A%E4%B8%83%E3%80%81%E7%95%8C%E7%B3%BB%E5%88%86%E5%88%AB%E9%97%A8">
		<outline text="随所生所系。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%9A%8F%E6%89%80%E7%94%9F%E6%89%80%E7%B3%BB%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊八、起灭分位门" _mubu_text="%E6%88%8A%E5%85%AB%E3%80%81%E8%B5%B7%E7%81%AD%E5%88%86%E4%BD%8D%E9%97%A8">
		<outline text="阿罗汉灭定，出世道无有。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%98%BF%E7%BD%97%E6%B1%89%E7%81%AD%E5%AE%9A%EF%BC%8C%E5%87%BA%E4%B8%96%E9%81%93%E6%97%A0%E6%9C%89%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text=" 丁三、了境能变" _mubu_text="%20%E4%B8%81%E4%B8%89%E3%80%81%E4%BA%86%E5%A2%83%E8%83%BD%E5%8F%98">
		<outline text="戊一、能变差别门" _mubu_text="%E6%88%8A%E4%B8%80%E3%80%81%E8%83%BD%E5%8F%98%E5%B7%AE%E5%88%AB%E9%97%A8">
		<outline text="次第三能变，差别有六种。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AC%A1%E7%AC%AC%E4%B8%89%E8%83%BD%E5%8F%98%EF%BC%8C%E5%B7%AE%E5%88%AB%E6%9C%89%E5%85%AD%E7%A7%8D%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊二、自性行相门" _mubu_text="%E6%88%8A%E4%BA%8C%E3%80%81%E8%87%AA%E6%80%A7%E8%A1%8C%E7%9B%B8%E9%97%A8">
		<outline text="了境为性相。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%86%E5%A2%83%E4%B8%BA%E6%80%A7%E7%9B%B8%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊三、三性分别门&nbsp;" _mubu_text="%E6%88%8A%E4%B8%89%E3%80%81%E4%B8%89%E6%80%A7%E5%88%86%E5%88%AB%E9%97%A8%C2%A0">
		<outline text="善不善俱非。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%96%84%E4%B8%8D%E5%96%84%E4%BF%B1%E9%9D%9E%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊四、相应俱受门" _mubu_text="%E6%88%8A%E5%9B%9B%E3%80%81%E7%9B%B8%E5%BA%94%E4%BF%B1%E5%8F%97%E9%97%A8">
		<outline text="己一、列名六位" _mubu_text="%E5%B7%B1%E4%B8%80%E3%80%81%E5%88%97%E5%90%8D%E5%85%AD%E4%BD%8D">
		<outline text="此心所遍行，别境善烦恼，随烦恼不定。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AD%A4%E5%BF%83%E6%89%80%E9%81%8D%E8%A1%8C%EF%BC%8C%E5%88%AB%E5%A2%83%E5%96%84%E7%83%A6%E6%81%BC%EF%BC%8C%E9%9A%8F%E7%83%A6%E6%81%BC%E4%B8%8D%E5%AE%9A%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="己二、受俱分别" _mubu_text="%E5%B7%B1%E4%BA%8C%E3%80%81%E5%8F%97%E4%BF%B1%E5%88%86%E5%88%AB">
		<outline text="皆三受相应。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%9A%86%E4%B8%89%E5%8F%97%E7%9B%B8%E5%BA%94%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="己三、重明六位" _mubu_text="%E5%B7%B1%E4%B8%89%E3%80%81%E9%87%8D%E6%98%8E%E5%85%AD%E4%BD%8D">
		<outline text="庚一、遍行" _mubu_text="%E5%BA%9A%E4%B8%80%E3%80%81%E9%81%8D%E8%A1%8C">
		<outline text="初遍行触等。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%88%9D%E9%81%8D%E8%A1%8C%E8%A7%A6%E7%AD%89%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚二、别境" _mubu_text="%E5%BA%9A%E4%BA%8C%E3%80%81%E5%88%AB%E5%A2%83">
		<outline text="次别境谓欲，胜解念定慧，所缘事不同。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AC%A1%E5%88%AB%E5%A2%83%E8%B0%93%E6%AC%B2%EF%BC%8C%E8%83%9C%E8%A7%A3%E5%BF%B5%E5%AE%9A%E6%85%A7%EF%BC%8C%E6%89%80%E7%BC%98%E4%BA%8B%E4%B8%8D%E5%90%8C%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚三、善" _mubu_text="%E5%BA%9A%E4%B8%89%E3%80%81%E5%96%84">
		<outline text="善谓信惭愧，无贪等三根，勤安不放逸，行舍及不害。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%96%84%E8%B0%93%E4%BF%A1%E6%83%AD%E6%84%A7%EF%BC%8C%E6%97%A0%E8%B4%AA%E7%AD%89%E4%B8%89%E6%A0%B9%EF%BC%8C%E5%8B%A4%E5%AE%89%E4%B8%8D%E6%94%BE%E9%80%B8%EF%BC%8C%E8%A1%8C%E8%88%8D%E5%8F%8A%E4%B8%8D%E5%AE%B3%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚四、烦恼" _mubu_text="%E5%BA%9A%E5%9B%9B%E3%80%81%E7%83%A6%E6%81%BC">
		<outline text="烦恼为贪嗔，痴慢疑恶见。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%83%A6%E6%81%BC%E4%B8%BA%E8%B4%AA%E5%97%94%EF%BC%8C%E7%97%B4%E6%85%A2%E7%96%91%E6%81%B6%E8%A7%81%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚五、随烦恼" _mubu_text="%E5%BA%9A%E4%BA%94%E3%80%81%E9%9A%8F%E7%83%A6%E6%81%BC">
		<outline text="随烦恼为忿，恨覆恼嫉懂，诳谄与害娇，无惭及无愧。掉举与昏沉　不信幷懈怠，放逸及失念，散乱不正知。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%9A%8F%E7%83%A6%E6%81%BC%E4%B8%BA%E5%BF%BF%EF%BC%8C%E6%81%A8%E8%A6%86%E6%81%BC%E5%AB%89%E6%87%82%EF%BC%8C%E8%AF%B3%E8%B0%84%E4%B8%8E%E5%AE%B3%E5%A8%87%EF%BC%8C%E6%97%A0%E6%83%AD%E5%8F%8A%E6%97%A0%E6%84%A7%E3%80%82%E6%8E%89%E4%B8%BE%E4%B8%8E%E6%98%8F%E6%B2%89%E3%80%80%E4%B8%8D%E4%BF%A1%E5%B9%B7%E6%87%88%E6%80%A0%EF%BC%8C%E6%94%BE%E9%80%B8%E5%8F%8A%E5%A4%B1%E5%BF%B5%EF%BC%8C%E6%95%A3%E4%B9%B1%E4%B8%8D%E6%AD%A3%E7%9F%A5%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚六、不定" _mubu_text="%E5%BA%9A%E5%85%AD%E3%80%81%E4%B8%8D%E5%AE%9A">
		<outline text="不定谓悔、眠，寻、伺二各二。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%AE%9A%E8%B0%93%E6%82%94%E3%80%81%E7%9C%A0%EF%BC%8C%E5%AF%BB%E3%80%81%E4%BC%BA%E4%BA%8C%E5%90%84%E4%BA%8C%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		</outline>
		<outline text="戊五、所依门" _mubu_text="%E6%88%8A%E4%BA%94%E3%80%81%E6%89%80%E4%BE%9D%E9%97%A8">
		<outline text="依止根本识。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BE%9D%E6%AD%A2%E6%A0%B9%E6%9C%AC%E8%AF%86%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊六、俱不俱转" _mubu_text="%E6%88%8A%E5%85%AD%E3%80%81%E4%BF%B1%E4%B8%8D%E4%BF%B1%E8%BD%AC">
		<outline text="五识随缘现，或俱或不俱，如涛波依水。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%94%E8%AF%86%E9%9A%8F%E7%BC%98%E7%8E%B0%EF%BC%8C%E6%88%96%E4%BF%B1%E6%88%96%E4%B8%8D%E4%BF%B1%EF%BC%8C%E5%A6%82%E6%B6%9B%E6%B3%A2%E4%BE%9D%E6%B0%B4%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊七、起灭分位门" _mubu_text="%E6%88%8A%E4%B8%83%E3%80%81%E8%B5%B7%E7%81%AD%E5%88%86%E4%BD%8D%E9%97%A8">
		<outline text="意识常现起，除生无想天，及无心二定，睡眠与闷绝。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%84%8F%E8%AF%86%E5%B8%B8%E7%8E%B0%E8%B5%B7%EF%BC%8C%E9%99%A4%E7%94%9F%E6%97%A0%E6%83%B3%E5%A4%A9%EF%BC%8C%E5%8F%8A%E6%97%A0%E5%BF%83%E4%BA%8C%E5%AE%9A%EF%BC%8C%E7%9D%A1%E7%9C%A0%E4%B8%8E%E9%97%B7%E7%BB%9D%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		</outline>
		<outline text="丙二、正辨唯识" _mubu_text="%E4%B8%99%E4%BA%8C%E3%80%81%E6%AD%A3%E8%BE%A8%E5%94%AF%E8%AF%86">
		<outline text="是诸识转变，分别所分别，由此彼皆无，故一切唯识。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%98%AF%E8%AF%B8%E8%AF%86%E8%BD%AC%E5%8F%98%EF%BC%8C%E5%88%86%E5%88%AB%E6%89%80%E5%88%86%E5%88%AB%EF%BC%8C%E7%94%B1%E6%AD%A4%E5%BD%BC%E7%9A%86%E6%97%A0%EF%BC%8C%E6%95%85%E4%B8%80%E5%88%87%E5%94%AF%E8%AF%86%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="丙三、通释妨难" _mubu_text="%E4%B8%99%E4%B8%89%E3%80%81%E9%80%9A%E9%87%8A%E5%A6%A8%E9%9A%BE">
		<outline text="丁一、释违理难" _mubu_text="%E4%B8%81%E4%B8%80%E3%80%81%E9%87%8A%E8%BF%9D%E7%90%86%E9%9A%BE">
		<outline text="戊一、心法生起缘由" _mubu_text="%E6%88%8A%E4%B8%80%E3%80%81%E5%BF%83%E6%B3%95%E7%94%9F%E8%B5%B7%E7%BC%98%E7%94%B1">
		<outline text="由一切种识，如是如是变，以辗转力故，彼彼分别生。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B1%E4%B8%80%E5%88%87%E7%A7%8D%E8%AF%86%EF%BC%8C%E5%A6%82%E6%98%AF%E5%A6%82%E6%98%AF%E5%8F%98%EF%BC%8C%E4%BB%A5%E8%BE%97%E8%BD%AC%E5%8A%9B%E6%95%85%EF%BC%8C%E5%BD%BC%E5%BD%BC%E5%88%86%E5%88%AB%E7%94%9F%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="戊二、有情相续缘由" _mubu_text="%E6%88%8A%E4%BA%8C%E3%80%81%E6%9C%89%E6%83%85%E7%9B%B8%E7%BB%AD%E7%BC%98%E7%94%B1">
		<outline text="由诸业习气，二取习气俱，前异熟既尽，复生余异熟。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B1%E8%AF%B8%E4%B8%9A%E4%B9%A0%E6%B0%94%EF%BC%8C%E4%BA%8C%E5%8F%96%E4%B9%A0%E6%B0%94%E4%BF%B1%EF%BC%8C%E5%89%8D%E5%BC%82%E7%86%9F%E6%97%A2%E5%B0%BD%EF%BC%8C%E5%A4%8D%E7%94%9F%E4%BD%99%E5%BC%82%E7%86%9F%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text=" 丁二、释违教难" _mubu_text="%20%E4%B8%81%E4%BA%8C%E3%80%81%E9%87%8A%E8%BF%9D%E6%95%99%E9%9A%BE">
		<outline text="戊一、三种自性" _mubu_text="%E6%88%8A%E4%B8%80%E3%80%81%E4%B8%89%E7%A7%8D%E8%87%AA%E6%80%A7">
		<outline text="己一、正辨" _mubu_text="%E5%B7%B1%E4%B8%80%E3%80%81%E6%AD%A3%E8%BE%A8">
		<outline text="庚一、遍计所执性" _mubu_text="%E5%BA%9A%E4%B8%80%E3%80%81%E9%81%8D%E8%AE%A1%E6%89%80%E6%89%A7%E6%80%A7">
		<outline text="由彼彼遍计，遍计种种物，此遍计所执，自性无所有。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B1%E5%BD%BC%E5%BD%BC%E9%81%8D%E8%AE%A1%EF%BC%8C%E9%81%8D%E8%AE%A1%E7%A7%8D%E7%A7%8D%E7%89%A9%EF%BC%8C%E6%AD%A4%E9%81%8D%E8%AE%A1%E6%89%80%E6%89%A7%EF%BC%8C%E8%87%AA%E6%80%A7%E6%97%A0%E6%89%80%E6%9C%89%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚二、依他起性" _mubu_text="%E5%BA%9A%E4%BA%8C%E3%80%81%E4%BE%9D%E4%BB%96%E8%B5%B7%E6%80%A7">
		<outline text="依他起自性，分别缘所生。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BE%9D%E4%BB%96%E8%B5%B7%E8%87%AA%E6%80%A7%EF%BC%8C%E5%88%86%E5%88%AB%E7%BC%98%E6%89%80%E7%94%9F%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚三、圆成实性" _mubu_text="%E5%BA%9A%E4%B8%89%E3%80%81%E5%9C%86%E6%88%90%E5%AE%9E%E6%80%A7">
		<outline text="圆成实于彼，常远离前性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%9C%86%E6%88%90%E5%AE%9E%E4%BA%8E%E5%BD%BC%EF%BC%8C%E5%B8%B8%E8%BF%9C%E7%A6%BB%E5%89%8D%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text="己二、明不一不异" _mubu_text="%E5%B7%B1%E4%BA%8C%E3%80%81%E6%98%8E%E4%B8%8D%E4%B8%80%E4%B8%8D%E5%BC%82">
		<outline text="故此与依他，非异非不异，如无常等性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%95%85%E6%AD%A4%E4%B8%8E%E4%BE%9D%E4%BB%96%EF%BC%8C%E9%9D%9E%E5%BC%82%E9%9D%9E%E4%B8%8D%E5%BC%82%EF%BC%8C%E5%A6%82%E6%97%A0%E5%B8%B8%E7%AD%89%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="己三、明依圆前后" _mubu_text="%E5%B7%B1%E4%B8%89%E3%80%81%E6%98%8E%E4%BE%9D%E5%9C%86%E5%89%8D%E5%90%8E">
		<outline text="非不见此彼。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%9D%9E%E4%B8%8D%E8%A7%81%E6%AD%A4%E5%BD%BC%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		<outline text="戊二、三种无性" _mubu_text="%E6%88%8A%E4%BA%8C%E3%80%81%E4%B8%89%E7%A7%8D%E6%97%A0%E6%80%A7">
		<outline text="己一、总说" _mubu_text="%E5%B7%B1%E4%B8%80%E3%80%81%E6%80%BB%E8%AF%B4">
		<outline text="即依此三性，立彼三无性，故佛密意说，一切法无性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8D%B3%E4%BE%9D%E6%AD%A4%E4%B8%89%E6%80%A7%EF%BC%8C%E7%AB%8B%E5%BD%BC%E4%B8%89%E6%97%A0%E6%80%A7%EF%BC%8C%E6%95%85%E4%BD%9B%E5%AF%86%E6%84%8F%E8%AF%B4%EF%BC%8C%E4%B8%80%E5%88%87%E6%B3%95%E6%97%A0%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="己二、别明" _mubu_text="%E5%B7%B1%E4%BA%8C%E3%80%81%E5%88%AB%E6%98%8E">
		<outline text="庚一、相无性" _mubu_text="%E5%BA%9A%E4%B8%80%E3%80%81%E7%9B%B8%E6%97%A0%E6%80%A7">
		<outline text="初即相无性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%88%9D%E5%8D%B3%E7%9B%B8%E6%97%A0%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚二、生无性" _mubu_text="%E5%BA%9A%E4%BA%8C%E3%80%81%E7%94%9F%E6%97%A0%E6%80%A7">
		<outline text="次无自然性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AC%A1%E6%97%A0%E8%87%AA%E7%84%B6%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="庚三、胜义无性" _mubu_text="%E5%BA%9A%E4%B8%89%E3%80%81%E8%83%9C%E4%B9%89%E6%97%A0%E6%80%A7">
		<outline text="后由远离前，所执我法性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%90%8E%E7%94%B1%E8%BF%9C%E7%A6%BB%E5%89%8D%EF%BC%8C%E6%89%80%E6%89%A7%E6%88%91%E6%B3%95%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
		</outline>
		</outline>
		</outline>
		</outline>
		</outline>
		<outline text="甲二、明唯识性" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B2%E4%BA%8C%E3%80%81%E6%98%8E%E5%94%AF%E8%AF%86%E6%80%A7%3C/span%3E" _heading="2">
		<outline text="此诸法胜义，亦即是真如，常如其性故，即唯识实性。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AD%A4%E8%AF%B8%E6%B3%95%E8%83%9C%E4%B9%89%EF%BC%8C%E4%BA%A6%E5%8D%B3%E6%98%AF%E7%9C%9F%E5%A6%82%EF%BC%8C%E5%B8%B8%E5%A6%82%E5%85%B6%E6%80%A7%E6%95%85%EF%BC%8C%E5%8D%B3%E5%94%AF%E8%AF%86%E5%AE%9E%E6%80%A7%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="甲三、明唯识位" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%94%B2%E4%B8%89%E3%80%81%E6%98%8E%E5%94%AF%E8%AF%86%E4%BD%8D%3C/span%3E" _heading="2">
		<outline text="乙一、资粮位" _mubu_text="%E4%B9%99%E4%B8%80%E3%80%81%E8%B5%84%E7%B2%AE%E4%BD%8D">
		<outline text="乃至未起识，求住唯识性，于二取随眠，犹未能伏灭。 " _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B9%83%E8%87%B3%E6%9C%AA%E8%B5%B7%E8%AF%86%EF%BC%8C%E6%B1%82%E4%BD%8F%E5%94%AF%E8%AF%86%E6%80%A7%EF%BC%8C%E4%BA%8E%E4%BA%8C%E5%8F%96%E9%9A%8F%E7%9C%A0%EF%BC%8C%E7%8A%B9%E6%9C%AA%E8%83%BD%E4%BC%8F%E7%81%AD%E3%80%82%20%3C/span%3E">
		</outline>
		</outline>
		<outline text="乙二、加行位" _mubu_text="%E4%B9%99%E4%BA%8C%E3%80%81%E5%8A%A0%E8%A1%8C%E4%BD%8D">
		<outline text="现前立少物，谓是唯识性，以有所得故，非实住唯识。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%8E%B0%E5%89%8D%E7%AB%8B%E5%B0%91%E7%89%A9%EF%BC%8C%E8%B0%93%E6%98%AF%E5%94%AF%E8%AF%86%E6%80%A7%EF%BC%8C%E4%BB%A5%E6%9C%89%E6%89%80%E5%BE%97%E6%95%85%EF%BC%8C%E9%9D%9E%E5%AE%9E%E4%BD%8F%E5%94%AF%E8%AF%86%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="乙三、通达位" _mubu_text="%E4%B9%99%E4%B8%89%E3%80%81%E9%80%9A%E8%BE%BE%E4%BD%8D">
		<outline text="若时于所缘，智都无所得，尔时住唯识，离二取相故。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%8B%A5%E6%97%B6%E4%BA%8E%E6%89%80%E7%BC%98%EF%BC%8C%E6%99%BA%E9%83%BD%E6%97%A0%E6%89%80%E5%BE%97%EF%BC%8C%E5%B0%94%E6%97%B6%E4%BD%8F%E5%94%AF%E8%AF%86%EF%BC%8C%E7%A6%BB%E4%BA%8C%E5%8F%96%E7%9B%B8%E6%95%85%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="乙四、修习位" _mubu_text="%E4%B9%99%E5%9B%9B%E3%80%81%E4%BF%AE%E4%B9%A0%E4%BD%8D">
		<outline text="无得不思议，是出世间智，舍二粗重故，便证得转依。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%97%A0%E5%BE%97%E4%B8%8D%E6%80%9D%E8%AE%AE%EF%BC%8C%E6%98%AF%E5%87%BA%E4%B8%96%E9%97%B4%E6%99%BA%EF%BC%8C%E8%88%8D%E4%BA%8C%E7%B2%97%E9%87%8D%E6%95%85%EF%BC%8C%E4%BE%BF%E8%AF%81%E5%BE%97%E8%BD%AC%E4%BE%9D%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		<outline text="乙五、究竟位" _mubu_text="%E4%B9%99%E4%BA%94%E3%80%81%E7%A9%B6%E7%AB%9F%E4%BD%8D">
		<outline text="此即无漏界，不思议善常，安乐解脱身，大牟尼名法。" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%AD%A4%E5%8D%B3%E6%97%A0%E6%BC%8F%E7%95%8C%EF%BC%8C%E4%B8%8D%E6%80%9D%E8%AE%AE%E5%96%84%E5%B8%B8%EF%BC%8C%E5%AE%89%E4%B9%90%E8%A7%A3%E8%84%B1%E8%BA%AB%EF%BC%8C%E5%A4%A7%E7%89%9F%E5%B0%BC%E5%90%8D%E6%B3%95%E3%80%82%3C/span%3E">
		</outline>
		</outline>
		</outline>
	</body>
</opml>